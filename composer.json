{
    "name": "wyrihaximus/monolog-processors",
    "description": "Collection of additional processors for monolog",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        }
    ],
    "require": {
        "php": "^8",
        "igorw/get-in": "^1.0.3",
        "monolog/monolog": "^2.9.1"
    },
    "require-dev": {
        "wyrihaximus/test-utilities": "^5.0.1"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\Monolog\\Processors\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\Tests\\Monolog\\Processors\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "icanhazstring/composer-unused": true,
            "ergebnis/composer-normalize": true
        },
        "platform": {
            "php": "8.0.12"
        },
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
